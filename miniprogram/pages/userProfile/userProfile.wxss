.content{
  background-color: #ffffff;
  height:100%;
}

.tab-container{
  display: flex;
  flex-direction: column;
}

.tab{
  height: 8vh;
  display: flex;
  flex-direction: row;
}

.icon-container{
  margin-top: 3.5%;
  margin-left: 3%;
}

.icon{
  height: 25px;
  width: 25px;
}

.tab-text{
  font-size: 15px;
  margin-left: 4%;
  margin-top: 4.5%;
}

.arrow{
  font-size: 20px;
  position: absolute;
  right: 10px;
  margin-top: 12px;
}

.seperate-line{
  height: 1px;
  background-color: #dddddd;
}

.head{
  height: 90px;
  display: flex;
  flex-direction: row;
}

.headIcon{
  margin-top: 15px;
  margin-left: 15px;
}

.headIcon image{
  width: 65px;
  height: 65px;
  border-radius: 50%;
}

.head text{
  color: #ffffff;
  font-size: 18px;
  position: absolute;
  left: 100px;
  margin-top: 35px;
}

.head button{
  position: fixed;
  left: 20%;
  width: 60%;
  margin-top: 5%;
  height: 40px;
  font-size: 18px;
  color: black;
  border-color: gray;
  background-color: #7bb32e;
  text-align: center;
  border-radius: 10px;
}


